SQL Queries

1. SELECT * FROM robots WHERE source = "Star Wars";

2. SELECT * FROM robots WHERE personality = "anxious";

3. SELECT * FROM recipes WHERE "nut_free" = TRUE;

4. SELECT COUNT (gluten_free) FROM recipes != "vegetarian" = TRUE;

5. SELECT * FROM animals WHERE "number_of_legs"(SELECTmax(int) FROM animals;

6. SELECT * FROM board_games WHERE "mins_to_play"(SELECTmin(int) FROM board_games);

7. SELECT * FROM recipes WHERE "minutes_required"(SELECTmax(int) FROM recipes);

8. SELECT * FROM robots WHERE "name" CHARINDEX('M', 1);

9. SELECT COUNT (board_games) WHERE "max_players" <=8;

10. SELECT * FROM animals WHERE "swimming" AND "egg_laying" = TRUE;

11. SELECT * FROM animals WHERE "swimming" AND "egg_laying" = TRUE AND "flying" = FALSE;

12. SELECT * FROM board_games WHERE "max_players" (SELECTmax(int) FROM board_games);


